<Window x:Class="ImpactAnalyzer.OptionsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Options" Height="300" Width="300" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" Loaded="Window_Loaded">
    <Grid x:Name="OptionsGrid" Background="Yellow">
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" Margin="127,232,0,0" VerticalAlignment="Top" Width="75" Click="Ok_Click" IsDefault="True"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="209,232,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click" IsCancel="True"/>

    </Grid>
</Window>
